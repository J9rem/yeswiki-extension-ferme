<div class="BAZ_rubrique field-{{ field.type}}" data-id="{{ field.propertyName }}">
    {% block label_container %}
        {#- No leading/trailing whitespace, see https://twig.symfony.com/doc/3.x/templates.html#templates-whitespace-control-#}
        <span class="BAZ_label">
            {%- block label -%}{{ _t('FERME_ACCESS_WIKI') }} :{%- endblock -%}
        </span>
    {% endblock %}
    {% block value_container %}
        <span class="BAZ_texte">
            {% block value %}
            <a class="btn btn-primary" href="{{ url }}" target="_blank">
            {{ url }}
            </a>
            {% endblock %}
        </span>
    {% endblock %}
</div>